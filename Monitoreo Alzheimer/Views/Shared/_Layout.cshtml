<!DOCTYPE html>
<html dir="ltr" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/assets/images/Logo ALZCare (formato PNG).png">
    <title>ALZCare</title>
    <!-- Custom CSS -->
    <link href="~/Content/assets/extra-libs/c3/c3.min.css" rel="stylesheet">
    <link href="~/Content/assets/libs/chartist/dist/chartist.min.css" rel="stylesheet">
    <link href="~/Content/assets/extra-libs/jvector/jquery-jvectormap-2.0.2.css" rel="stylesheet" />
    <!-- Custom CSS -->
    <link href="~/Content/dist/css/style.min.css" rel="stylesheet">
    <link href="~/Content/assets/extra-libs/datatables.net-bs4/css/dataTables.bootstrap4.css" rel="stylesheet">

    
</head>

<body>
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div class="preloader">
        <div class="lds-ripple">
            <div class="lds-pos"></div>
            <div class="lds-pos"></div>
        </div>
    </div>


    <div id="main-wrapper" data-theme="light" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
         data-sidebar-position="fixed" data-header-position="fixed" data-boxed-layout="full">
        <!-- ============================================================== -->
        <!-- Topbar header - style you can find in pages.scss -->
        <!-- ============================================================== -->
        <header class="topbar" data-navbarbg="skin6">
            <nav class="navbar top-navbar navbar-expand-md">
                <div class="navbar-header" data-logobg="skin6">
                    <!-- This is for the sidebar toggle which is visible on mobile only -->
                    <a class="nav-toggler waves-effect waves-light d-block d-md-none" href="javascript:void(0)">
                        <i class="ti-menu ti-close"></i>
                    </a>
                    <!-- ============================================================== -->
                    <!-- Logo -->
                    <!-- ============================================================== -->
                    <div>
                        <!-- Logo icon -->

                        <a href="#">
                            <center>

                                <b class="logo-icon">

                                    <!-- Dark Logo icon -->
                                    <img src="~/Content/assets/images/Logo ALZCare (formato PNG).png" alt="homepage" width="100" height="100" class="dark-logo" style="display:block;margin:auto;margin-top:20px" />
                                    <!-- Light Logo icon -->

                                </b>
                            </center>
                            <!--End Logo icon -->
                            <!-- Logo text -->
                            @*<span class="logo-text">

                                    <img src="~/Content/assets/images/logo-text.png" alt="homepage" class="dark-logo" />


                                </span>*@
                        </a>


                    </div>

                    <a class="topbartoggler d-block d-md-none waves-effect waves-light" href="javascript:void(0)"
                       data-toggle="collapse" data-target="#navbarSupportedContent"
                       aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <i class="ti-more"></i>
                    </a>
                </div>
                <!-- ============================================================== -->
                <!-- End Logo -->
                <!-- ============================================================== -->
                <div class="navbar-collapse collapse" id="navbarSupportedContent">
                    <!-- ============================================================== -->
                    <!-- Right side toggle and nav items -->

                </div>


            </nav>
        </header>


        <aside class="left-sidebar" data-sidebarbg="skin6">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar" data-sidebarbg="skin6">
                <!-- Sidebar navigation-->
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">
                        <li class="sidebar-item">
                            <a class="sidebar-link sidebar-link" href='@Url.Action("Perfil","Medico")'
                               aria-expanded="false">
                                <i data-feather="user" class="feather-icon"></i><span class="hide-menu">Mi Perfil</span>
                            </a>
                        </li>
                        <li class="list-divider"></li>
                        <li class="nav-small-cap"><span class="hide-menu">Gestión médica</span></li>


                        <li class="sidebar-item">
                            <a class="sidebar-link sidebar-link" href='@Url.Action("ListaPacientes","Medico")'
                               aria-expanded="false">
                                <i data-feather="grid" class="feather-icon"></i><span class="hide-menu">Lista de pacientes</span>
                            </a>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link sidebar-link" href='@Url.Action("ListaAlertas","Medico")'
                               aria-expanded="false">
                                <i data-feather="message-square" class="feather-icon"></i><span class="hide-menu">Notificaciones</span>
                            </a>
                        </li>

                        @if (User.Identity.IsAuthenticated)
                        {
                            <li class="sidebar-item">
                                <a class="sidebar-link sidebar-link" href='@Url.Action("CerrarSesion","Medico")'
                                   aria-expanded="false">
                                    <i data-feather="log-out" class="feather-icon"></i><span class="hide-menu">Cerrar sesion</span>
                                </a>
                            </li>
                        }

                    </ul>
                </nav>
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
        </aside>

        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="page-breadcrumb">
                <div class="container-fluid">

                    @RenderBody()

                </div>

                <footer class="footer text-center text-muted">
                  ALZCare
                </footer>
                <!-- ============================================================== -->
                <!-- End footer -->
                <!-- ============================================================== -->
            </div>
            <!-- ============================================================== -->
            <!-- End Page wrapper  -->
            <!-- ============================================================== -->
        </div>
    </div>
    
    <script src="~/Content/assets/libs/jquery/dist/jquery.min.js"></script>
    <script src="~/Content/assets/libs/popper.js/dist/umd/popper.min.js"></script>
    <script src="~/Content/assets/libs/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- apps -->
    <script src="~/Content/sweetalert.min.js"></script>

    <!-- apps -->
    <script src="~/Content/dist/js/app-style-switcher.js"></script>
    <script src="~/Content/dist/js/feather.min.js"></script>
    <script src="~/Content/assets/libs/perfect-scrollbar/dist/perfect-scrollbar.jquery.min.js"></script>
    <script src="../dist/js/sidebarmenu.js"></script>
    <!--Custom JavaScript -->
    <script src="~/Content/dist/js/custom.min.js"></script>
    <!--This page JavaScript -->
    <script src="~/Content/assets/extra-libs/c3/d3.min.js"></script>
    <script src="~/Content/assets/extra-libs/c3/c3.min.js"></script>
    <script src="~/Content/assets/libs/chartist/dist/chartist.min.js"></script>
    <script src="~/Content/assets/libs/chartist-plugin-tooltips/dist/chartist-plugin-tooltip.min.js"></script>
    <script src="~/Content/assets/extra-libs/jvector/jquery-jvectormap-2.0.2.min.js"></script>
    <script src="~/Content/assets/extra-libs/jvector/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/Content/dist/js/pages/dashboards/dashboard1.min.js"></script>
    <script src="~/Content/assets/extra-libs/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/dist/js/pages/datatable/datatable-basic.init.js"></script>

    <script src="~/Content/assets/extra-libs/sparkline/sparkline.js"></script>
    <!--Wave Effects -->
    <!-- themejs -->
    <!--Menu sidebar -->
    <script src="~/Content/dist/js/sidebarmenu.js"></script>
    <!--Custom JavaScript -->
    <script src="~/Content/dist/js/custom.min.js"></script>



</body>


</html>